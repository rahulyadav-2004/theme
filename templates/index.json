    "custom_html_eHcXmq": {
      "type": "custom-html",
      "name": "Custom HTML",
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": true,
        "remove_horizontal_spacing": true,
        "html": "<style>\n  .feen-section {\n    width: 100%;\n    padding: 20px 24px;\n    background: transparent;\n    box-sizing: border-box;\n    font-family: -apple-system, BlinkMacSystemFont, \"SF Pro Display\", Roboto, sans-serif;\n  }\n\n  .feen-header {\n    display: flex;\n    align-items: center;\n    gap: 12px;\n    margin-bottom: 12px;\n  }\n\n  .feen-header img {\n    width: 40px;\n    height: 40px;\n    object-fit: contain;\n  }\n\n  .feen-header h2 {\n    font-size: 32px;\n    font-weight: 700;\n    margin: 0;\n    text-transform: uppercase;\n    background: linear-gradient(270deg, #0047AB, #003D99, #0047AB);\n    background-size: 800% 100%;\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n    animation: pulseDark 4s linear infinite;\n  }\n\n  .feen-subheading {\n    font-size: 18px;\n    color: #1e3a8a; /* deep blue */\n    margin-bottom: 32px;\n    font-weight: 500;\n  }\n\n  @keyframes pulseDark {\n    0% { background-position: 0% 50%; }\n    50% { background-position: 100% 50%; }\n    100% { background-position: 0% 50%; }\n  }\n\n  .feen-scroll {\n    display: flex;\n    gap: 20px;\n    overflow-x: auto;\n    scroll-snap-type: x mandatory;\n    -webkit-overflow-scrolling: touch;\n    padding-bottom: 8px;\n    justify-content: flex-start;\n  }\n\n  .feen-card {\n    flex: 0 0 auto;\n    width: 200px;\n    scroll-snap-align: start;\n    background: #111827;\n    border: 1px solid black;\n    border-radius: 20px;\n    padding: 24px 16px;\n    text-align: center;\n  }\n\n  .feen-card img {\n    width: 100px;\n    height: 100px;\n    object-fit: contain;\n    margin: 0 auto 16px auto;\n    display: block;\n    filter: brightness(0) invert(1);\n  }\n\n  .feen-card .name {\n    font-size: 17px;\n    font-weight: 600;\n    color: #ffffff;\n    margin-bottom: 6px;\n  }\n\n  .feen-card .desc {\n    font-size: 14px;\n    color: #d1d5db;\n    line-height: 1.5;\n  }\n\n  .feen-scroll::-webkit-scrollbar {\n    height: 6px;\n  }\n\n  .feen-scroll::-webkit-scrollbar-thumb {\n    background: #444;\n    border-radius: 8px;\n  }\n\n  @media (min-width: 768px) {\n    .feen-scroll {\n      overflow-x: visible;\n      flex-wrap: nowrap;\n      justify-content: space-between;\n    }\n\n    .feen-card {\n      width: 220px;\n    }\n  }\n</style>\n\n<div class=\"feen-section\">\n  <div class=\"feen-header\">\n    <img src=\"https://cdn.shopify.com/s/files/1/0753/3169/8944/files/salty-food-in-a-bowl.png?v=1749072664\" alt=\"Feen Logo\">\n    <h2>INSIDE FEEN</h2>\n  </div>\n  <div class=\"feen-subheading\">What are Feen pouches made of?</div>\n\n  <div class=\"feen-scroll\">\n    <div class=\"feen-card\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0753/3169/8944/files/image_2.png?v=1749072367\" alt=\"Nicotine\">\n      <div class=\"name\">Nicotine</div>\n      <div class=\"desc\">Pharma-grade, tobacco-free blend.</div>\n    </div>\n    <div class=\"feen-card\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0753/3169/8944/files/image_3.png?v=1749072468\" alt=\"Flavour\">\n      <div class=\"name\">Flavour</div>\n      <div class=\"desc\">Crafted for a crisp and lasting experience.</div>\n    </div>\n    <div class=\"feen-card\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0753/3169/8944/files/spices.png?v=1745994772\" alt=\"Sweeteners\">\n      <div class=\"name\">Sweeteners</div>\n      <div class=\"desc\">Food-safe, no sugar, clean finish.</div>\n    </div>\n    <div class=\"feen-card\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0753/3169/8944/files/vegan.png?v=1745994787\" alt=\"pH Control\">\n      <div class=\"name\">pH Control</div>\n      <div class=\"desc\">Ensures optimal delivery and feel.</div>\n    </div>\n    <div class=\"feen-card\">\n      <img src=\"https://cdn.shopify.com/s/files/1/0753/3169/8944/files/extract.png?v=1745994732\" alt=\"Stabilizers\">\n      <div class=\"name\">Stabilizers</div>\n      <div class=\"desc\">Keeps pouch fresh and long-lasting.</div>\n    </div>\n  </div>\n</div>",
        "background": "#ffffff",
        "background_gradient": "",
        "text_color": ""
      }
    },
